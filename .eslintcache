tify-3'\r\n\r\nimport {\r\n  loginUserRequest,\r\n  logoutUserRequest,\r\n  refreshTokensLogin,\r\n  registrationUserRequest\r\n} from '@/api/requests'\r\n\r\nimport type {\r\n  AuthResponse,\r\n  InitialValuesUserStore,\r\n  RefreshTokensLoginResponse,\r\n  RegistrationUserActionProps\r\n} from './types/userStoreTypes'\r\nimport type { RegistrationUserData, LoginUserData } from '@/api/requests/types'\r\nimport { useRouter } from 'vue-router'\r\n\r\nexport const useUserStore = defineStore('user', () => {\r\n  const router = useRouter()\r\n  const { toastify } = useToastify()\r\n  const userState = ref<InitialValuesUserStore>({\r\n    user: null\r\n  })\r\n\r\n  async function registrationUser({\r\n    data,\r\n    currentStepIdx,\r\n    isShowEmailConfirmMessage,\r\n    uploadedAvatar,\r\n    resetForm\r\n  }: RegistrationUserActionProps) {\r\n    try {\r\n      const {\r\n        data: { user, accessToken, refreshToken }\r\n      }: AuthResponse = await registrationUserRequest(data)\r\n\r\n      if (user) {\r\n        userState.value.user = user\r\n        localStorage.setItem('accessToken', accessToken)\r\n        localStorage.setItem('refreshToken', refreshToken)\r\n        currentStepIdx.value = 0\r\n        uploadedAvatar.value.photo = null\r\n        resetForm()\r\n      }\r\n      if (!user?.isEmailConfirmed) {\r\n        isShowEmailConfirmMessage.value = true\r\n      }\r\n    } catch (e) {\r\n      console.log(2222)\r\n      toastify('error', )\r\n      console.error(e)\r\n    }\r\n  }\r\n\r\n  async function loginUser(data: LoginUserData, resetForm: any) {\r\n    try {\r\n      const {\r\n        data: { user, accessToken, refreshToken }\r\n      }: AuthResponse = await loginUserRequest(data)\r\n\r\n      if (user) {\r\n        userState.value.user = user\r\n        localStorage.setItem('accessToken', accessToken)\r\n        localStorage.setItem('refreshToken', refreshToken)\r\n        router.push({ name: 'wallet' })\r\n        resetForm()\r\n      }\r\n    } catch (e) {\r\n      console.error(e)\r\n    }\r\n  }\r\n\r\n  async function logoutUser() {\r\n    try {\r\n      const accessToken = localStorage.getItem('accessToken')\r\n      if (accessToken) {\r\n        await logoutUserRequest(accessToken)\r\n        localStorage.clear()\r\n        userState.value.user = null\r\n        router.push({ path: '/' })\r\n      }\r\n    } catch (e) {\r\n      console.error(e)\r\n    }\r\n  }\r\n\r\n  async function checkAuth() {\r\n    try {\r\n      const refreshToken = localStorage.getItem('refreshToken')\r\n      if (!refreshToken) {\r\n        throw new Error()\r\n      }\r\n      const {\r\n        data: { user, tokens }\r\n      }: RefreshTokensLoginResponse = await refreshTokensLogin(refreshToken)\r\n\r\n      localStorage.setItem('accessToken', tokens.accessToken)\r\n      localStorage.setItem('refreshToken', tokens.refreshToken)\r\n      userState.value.user = user\r\n      router.push({ name: 'wallet' })\r\n    } catch (e) {\r\n      router.push({ path: '/' })\r\n      console.error(e)\r\n    }\r\n  }\r\n\r\n  return {\r\n    userState,\r\n    registrationUser,\r\n    loginUser,\r\n    logoutUser,\r\n    checkAuth\r\n  }\r\n})\r\n","C:\\Progects\\wallet-client\\src\\components\\UI\\wrappers\\AuthWrapper.vue",[],[],"C:\\Progects\\wallet-client\\src\\components\\Header\\Header.vue",[],[],"C:\\Progects\\wallet-client\\src\\components\\notifications\\AuthGreeting.vue",[],[],"C:\\Progects\\wallet-client\\src\\components\\forms\\mainForms\\SignUp.vue",["208"],[],"<template>\r\n  <form class=\"sign-up-form\" @submit=\"onSubmit\" v-if=\"!isShowEmailConfirmMessage\">\r\n    <ProgressForm :active-step=\"currentStepIdx + 1\" />\r\n\r\n    <FormStep>\r\n      <CustomFormInput\r\n        :value=\"values.firstName\"\r\n        name=\"firstName\"\r\n        type=\"text\"\r\n        label=\"First name\"\r\n        placeholder=\"Input your first name\"\r\n      />\r\n      <CustomFormInput\r\n        :value=\"values.lastName\"\r\n    