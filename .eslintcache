onfirmMessage\" :email=\"userStore.userState.user?.email\" />\n</template>\n\n<script setup lang=\"ts\">\nimport FormStep from './steps/FormStep.vue'\nimport ProgressForm from './ProgressForm.vue'\nimport UploadAvatarStep from './steps/UploadAvatarStep.vue'\nimport RegistrationFormNavigate from 'navigation/RegistrationFormNavigate.vue'\nimport ConfirmEmailAddress from 'components/notifications/ConfirmEmailAddress.vue'\n\nimport validationSchema from '@/utils/validate/registrationValidateSchema'\nimport { useUserStore } from '@/store/userStore'\n\nimport { ref, computed, provide } from 'vue'\nimport { useToastify } from 'vue-toastify-3'\nimport { useForm } from 'vee-validate'\n\nimport { type InitialValuesSignUpForm, type RegistrationData } from './types'\n\nconst userStore = useUserStore()\nconst { toastify } = useToastify()\n\nconst currentStepIdx = ref(0)\nconst stepCounter = ref(0)\nconst uploadedAvatar = ref<{ photo: File | null }>({ photo: null })\nconst isShowEmailConfirmMessage = ref<boolean>(false)\n\nprovide('STEP_COUNTER', stepCounter)\nprovide('CURRENT_STEP_INDEX', currentStepIdx)\n\nconst currentSchema = computed(() => {\n  return validationSchema[currentStepIdx.value]\n})\n\nconst { values, handleSubmit } = useForm<InitialValuesSignUpForm>({\n  validationSchema: currentSchema,\n  keepValuesOnUnmount: true\n})\n\nconst isLastStep = computed(() => {\n  return currentStepIdx.value === stepCounter.value - 1\n})\n\nconst hasPrevious = computed(() => {\n  return currentStepIdx.value > 0\n})\n\nconst onSubmit = handleSubmit(async (values, { resetForm }) => {\n  if (!isLastStep.value) {\n    currentStepIdx.value++\n    return\n  }\n\n  if (uploadedAvatar.value.photo) {\n    const { confirmPassword, ...data }: RegistrationData = {\n      ...values,\n      photo: uploadedAvatar.value.photo\n    }\n    await userStore.registrationUser({\n      data,\n      currentStepIdx,\n      uploadedAvatar,\n      isShowEmailConfirmMessage,\n      resetForm\n    })\n  } else {\n    toastify('warning', 'Avatar is required field')\n  }\n})\n\nfunction goToPrev() {\n  if (currentStepIdx.value === 0) {\n    return\n  }\n\n  currentStepIdx.value--\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.sign-up-form {\n  width: 720px;\n  max-height: 380px;\n  margin: 0 20px;\n}\n</style>\n","/Users/user/Desktop/Projects/wallet-client/src/components/items/WidgetBalanceItem.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/sidebars/user-sidebar/UserBills.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/forms/widgetForms/CorrectBalanceForm.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/widgets/widget-panel/WidgetPanel.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/widgets/CoastStructureWidget.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/ui/inputs/RadioButtonInputGroup.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/notifications/MainPageGreeting.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/notifications/AuthGreeting.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/forms/widgetForms/EditUserForm.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/forms/widgetForms/CreditCartForm.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/api/requests/index.ts",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/ui/accordion/Accordion.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/forms/widgetForms/UpdateOperationForm.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/items/WidgetCategoryItem.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/components/notifications/ConfirmEmailAddress.vue",[],[],"/Users/user/Desktop/Projects/wallet-client/src/utils/validate/authValidateSchema.ts",[],[],"/Users/user/Desktop/Projects/wallet-client/src/store/userStore.ts",[],[],"/Users/user/Desktop/Projects/wallet-clie